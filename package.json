{
	"name": "sol-dev-env",
	"version": "1.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"contracts/*",
		"packages/*"
	],
	"scripts": {
		"contract:build:helloworld": "cargo build-bpf --manifest-path=contracts/helloworld/Cargo.toml --bpf-out-dir=dist/solana/helloworld"
	},
	"prettier": {
		"tabWidth": 4,
		"semi": true,
		"singleQuote": true,
		"useTabs": true,
		"printWidth": 200
	},
	"packageManager": "yarn@4.0.0-rc.18"
}
